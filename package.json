  "xo": {
    "env": [
      "browser",
      "node",
      "jquery"
    ],
    "space": true,
    "globals": [
      "Debug",
      "Overlay",
      "Gingabulous",
      "Helper"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        2,
        {
          "outerIIFEBody": 0
        }
      ],
      "wrap-iife": [
        "error",
        "any"
      ],
      "key-spacing": [
        "error",
        {
          "align": "value"
        }
      ],
      "no-multi-spaces": [
        "error",
        {
          "exceptions": {
            "Property": true,
            "VariableDeclarator": true
          }
        }
      ],
      "quote-props": [
        "error",
        "consistent-as-needed"
      ]
    }
  }
}
